{
  "name": "@driveway/internal-ui-template-nextjs",
  "version": "0.1.0",
  "description": "Driveway Internal-UI-Template built with React & NextJS.",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "cross-env NODE_OPTIONS='--inspect' NEXT_PUBLIC_ENVIRONMENT=local PORT=8080 next dev",
    "start": "PORT=8080 next start",
    "build": "next build",
    "lint": "tsc && eslint --cache --ext .ts,.tsx ./src",
    "lint:fix": "tsc && eslint --cache --fix --ext .ts,.tsx ./src",
    "lint-staged": "lint-staged",
    "test": "TZ=utc jest --watch --maxWorkers=50%",
    "test:ci": "TZ=utc CI=true jest --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --maxWorkers=50%",
    "prepare": "husky install"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.21.0",
    "@azure/msal-common": "^7.3.0",
    "@azure/msal-react": "^1.2.0",
    "@datadog/browser-logs": "^4.19.1",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.8.8",
    "@iconify/react": "^3.1.0",
    "@mui/base": "^5.0.0-alpha.93",
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "^5.10.7",
    "@mui/system": "^5.4.4",
    "@next/bundle-analyzer": "^12.3.1",
    "@optimizely/optimizely-sdk": "^4.9.2",
    "@optimizely/react-sdk": "^2.9.1",
    "@tanstack/react-query": "^4.7.1",
    "@types/nanoid": "^2.1.0",
    "change-case": "^4.1.2",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.27.0",
    "dayjs": "^1.11.0",
    "dd-trace": "^3.3.1",
    "framer-motion": "^7.2.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "next": "^12.3.1",
    "notistack": "^2.0.3",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "pg": "^8.9.0",
    "pino": "^8.6.0",
    "pino-http": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.26.0",
    "react-intersection-observer": "^9.4.0",
    "react-lazy-load-image-component": "^1.5.1",
